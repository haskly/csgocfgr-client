            {{#keyboard-key class="Keyboard-key" modelVal=data.keyK}}k{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyL}}l{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keySemicolon}};{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyApostrophe}}&#39;{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--rt" modelVal=data.keyEnter}}&#8629;{{/keyboard-key}}
          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key Keyboard-key--sft" modelVal=data.keyShift}}&#8679;{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyZ}}z{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyX}}x{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyC}}c{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyV}}v{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyB}}b{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyN}}n{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyM}}m{{/keyboard-key}}
