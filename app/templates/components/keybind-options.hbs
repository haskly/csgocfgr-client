            {{#keyboard-key class="Keyboard-key" modelVal=data.keyO}}o{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyP}}p{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyLbracket}}]{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyRbracket}}[{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--sb" modelVal=data.keyBkslash}}\\{{/keyboard-key}}
          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key Keyboard-key--cp" modelVal=data.keyCapslock}}Caps{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyA}}a{{/keyboard-key}}
