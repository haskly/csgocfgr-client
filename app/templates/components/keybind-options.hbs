            <button class="Keyboard-key" {{action 'openModal' 'keyKpRightarrow' keyKpRightarrow model}}>6</button>
          </div>
          <div class="Keyboard-row">
            <button class="Keyboard-key" {{action 'openModal' 'keyKpEnd' keyKpEnd model}}>1</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyKpDownarrow' keyKpDownarrow model}}>2</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyKpPgdn' keyKpPgdn model}}>3</button>
            <button class="Keyboard-key Keyboard-key--ent" {{action 'openModal' 'keyKpEnter' keyKpEnter model}}>Enter</button>
          </div>
          <div class="Keyboard-row">
            <button class="Keyboard-key Keyboard-key--zd" {{action 'openModal' 'keyKpIns' keyKpIns model}}>0</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyKpDel' keyKpDel model}}>.</button>
          </div>
        </div>
        <div class="Keyboard-mouse">
          <div class="Keyboard-row">
            <button class="Keyboard-key Keyboard-key--lmb" {{action 'openModal' 'keyMouse1' keyMouse1 model}}>MB1</button>
            <div class="Keyboard-row Keyboard-row--mm">
              <button class="Keyboard-key Keyboard-key--mmbu" {{action 'openModal' 'keyMwheelup' keyMwheelup model}}>&#8593;</button>
              <button class="Keyboard-key Keyboard-key--mmbc" {{action 'openModal' 'keyMouse3' keyMouse3 model}}>MB3</button>
              <button class="Keyboard-key Keyboard-key--mmbd" {{action 'openModal' 'keyMwheeldown' keyMwheeldown model}}>&#8595;</button>
