            <button class="Keyboard-key" {{action 'openModal' 'keyH' keyH model}}>h</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyJ' keyJ model}}>j</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyK' keyK model}}>k</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyL' keyL model}}>l</button>
            <button class="Keyboard-key" {{action 'openModal' 'keySemicolon' keySemicolon model}}>;</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyApostrophe' keyApostrophe model}}>&#39;</button>
            <button class="Keyboard-key Keyboard-key--rt" {{action 'openModal' 'keyEnter' keyEnter model}}>&#8629;</button>
          </div>
          <div class="Keyboard-row">
            <button class="Keyboard-key Keyboard-key--sft" {{action 'openModal' 'keyShift' keyShift model}}>&#8679;</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyZ' keyZ model}}>z</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyX' keyX model}}>x</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyC' keyC model}}>c</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyV' keyV model}}>v</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyB' keyB model}}>b</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyN' keyN model}}>n</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyM' keyM model}}>m</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyComma' keyComma model}}>,</button>
