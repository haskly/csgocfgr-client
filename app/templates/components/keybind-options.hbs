          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key Keyboard-key--cl Keyboard-key--pr2" modelVal=data.keyCtrl}}Ctrl{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--at" modelVal=data.keyAlt}}Alt{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--sp" modelVal=data.keySpace}}Space{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--at Keyboard-key--pr3" modelVal=data.keyAlt}}Alt{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--cl" modelVal=data.keyCtrl}}Ctrl{{/keyboard-key}}
          </div>
        </div>
        <div class="Keyboard-arrows">
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key Keyboard-key--hidden"}}{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--hidden"}}{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyPause}}Pause{{/keyboard-key}}
          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyIns}}Ins{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyHome}}Home{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyPgup}}PgUp{{/keyboard-key}}
          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyDel}}Del{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyEnd}}End{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyPgdn}}PgDn{{/keyboard-key}}
          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key Keyboard-key--hidden"}}{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--hidden"}}{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--hidden"}}{{/keyboard-key}}
          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key Keyboard-key--hidden"}}{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyUparrow}}&#8593;{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--hidden"}}{{/keyboard-key}}
          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyLeftarrow}}&#8592;{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyDownarrow}}&#8595;{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyRightarrow}}&#8594;{{/keyboard-key}}
          </div>
        </div>
        <div class="Keyboard-tenkey">
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key Keyboard-key--hidden"}}{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--hidden"}}{{/keyboard-key}}
