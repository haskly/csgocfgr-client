            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpLeftarrow}}4{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKp5}}5{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpRightarrow}}6{{/keyboard-key}}
          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpEnd}}1{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpDownarrow}}2{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpPgdn}}3{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--ent" modelVal=data.keyKpEnter}}Enter{{/keyboard-key}}
          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key Keyboard-key--zd" modelVal=data.keyKpIns}}0{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpDel}}.{{/keyboard-key}}
          </div>
        </div>
        <div class="Keyboard-mouse">
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key Keyboard-key--lmb" modelVal=data.keyMouse1}}MB1{{/keyboard-key}}
            <div class="Keyboard-row Keyboard-row--mm">
              {{#keyboard-key class="Keyboard-key Keyboard-key--mmbu" modelVal=data.keyMwheelup}}&#8593;{{/keyboard-key}}
              {{#keyboard-key class="Keyboard-key Keyboard-key--mmbc" modelVal=data.keyMouse3}}MB3{{/keyboard-key}}
              {{#keyboard-key class="Keyboard-key Keyboard-key--mmbd" modelVal=data.keyMwheeldown}}&#8595;{{/keyboard-key}}
            </div>
            {{#keyboard-key class="Keyboard-key Keyboard-key--rmb" modelVal=data.keyMouse2}}MB2{{/keyboard-key}}
          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key Keyboard-key--mb4" modelVal=data.keyMouse4}}MB4{{/keyboard-key}}
