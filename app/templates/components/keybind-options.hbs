            <button class="Keyboard-key Keyboard-key--cl Keyboard-key--pr2" {{action 'openModal' 'keyCtrl' keyCtrl model}}>Ctrl</button>
            <button class="Keyboard-key Keyboard-key--at" {{action 'openModal' 'keyAlt' keyAlt model}}>Alt</button>
            <button class="Keyboard-key Keyboard-key--sp" {{action 'openModal' 'keySpace' keySpace model}}>Space</button>
            <button class="Keyboard-key Keyboard-key--at Keyboard-key--pr3" {{action 'openModal' 'keyAlt' keyAlt model}}>Alt</button>
            <button class="Keyboard-key Keyboard-key--cl" {{action 'openModal' 'keyCtrl' keyCtrl model}}>Ctrl</button>
          </div>
        </div>
        <div class="Keyboard-arrows">
          <div class="Keyboard-row">
            <button class="Keyboard-key Keyboard-key--hidden"></button>
            <button class="Keyboard-key Keyboard-key--hidden"></button>
            <button class="Keyboard-key" {{action 'openModal' 'keyPause' keyPause model}}>Pause</button>
          </div>
          <div class="Keyboard-row">
            <button class="Keyboard-key" {{action 'openModal' 'keyIns' keyIns model}}>Ins</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyHome' keyHome model}}>Home</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyPgup' keyPgup model}}>PgUp</button>
          </div>
          <div class="Keyboard-row">
            <button class="Keyboard-key" {{action 'openModal' 'keyDel' keyDel model}}>Del</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyEnd' keyEnd model}}>End</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyPgdn' keyPgdn model}}>PgDn</button>
          </div>
          <div class="Keyboard-row">
            <button class="Keyboard-key Keyboard-key--hidden"></button>
            <button class="Keyboard-key Keyboard-key--hidden"></button>
            <button class="Keyboard-key Keyboard-key--hidden"></button>
          </div>
          <div class="Keyboard-row">
            <button class="Keyboard-key Keyboard-key--hidden"></button>
            <button class="Keyboard-key" {{action 'openModal' 'keyUparrow' keyUparrow model}}>&#8593;</button>
            <button class="Keyboard-key Keyboard-key--hidden"></button>
          </div>
          <div class="Keyboard-row">
            <button class="Keyboard-key" {{action 'openModal' 'keyLeftarrow' keyLeftarrow model}}>&#8592;</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyDownarrow' keyDownarrow model}}>&#8595;</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyRightarrow' keyRightarrow model}}>&#8594;</button>
          </div>
        </div>
        <div class="Keyboard-tenkey">
          <div class="Keyboard-row">
            <button class="Keyboard-key" class="Keyboard-key Keyboard-key--hidden"></button>
            <button class="Keyboard-key" class="Keyboard-key Keyboard-key--hidden"></button>
            <button class="Keyboard-key" class="Keyboard-key Keyboard-key--hidden"></button>
            <button class="Keyboard-key" class="Keyboard-key Keyboard-key--hidden"></button>
          </div>
          <div class="Keyboard-row">
            <button class="Keyboard-key Keyboard-key--disabled" disabled="disabled">Num Lock</button>
            <button class="Keyboard-key" {{action 'openModal' 'keyKpSlash' keyKpSlash model}}>/</button>
