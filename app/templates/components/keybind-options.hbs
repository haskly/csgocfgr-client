            {{#keyboard-key class="Keyboard-key Keyboard-key--disabled" disabled="disabled"}}NumLk{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpSlash}}/{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpMultiply}}*{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpMinus}}-{{/keyboard-key}}
          </div>
          <div class="Keyboard-row">
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpHome}}7{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpUparrow}}8{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key" modelVal=data.keyKpPgup}}9{{/keyboard-key}}
            {{#keyboard-key class="Keyboard-key Keyboard-key--pl" modelVal=data.keyKpPlus}}+{{/keyboard-key}}
          </div>
