            <button class="Keyboard-key" {{action 'openModal' 'keyKpPgup' keyKpPgup model}}>9</button>
            <button class="Keyboard-key Keyboard-key--pl" {{action 'openModal' 'keyKpPlus' keyKpPlus model}}>+</button>
